{
  "expo": {
    "name": "PTCV-React-Native",
    "slug": "PTCV-React-Native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "sendos.ia",
    "splash": {
      "image": "./assets/iconosendos.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconosendos.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.example.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "63053a58-ffac-48da-8108-3e6053741391"
      }
    }
  }
}
